{
    "name": "@feature-flag-extractor/angular-extractor",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/feature-flag-extractor/angular-extractor/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/feature-flag-extractor/angular-extractor",
                "main": "libs/feature-flag-extractor/angular-extractor/src/index.ts",
                "tsConfig": "libs/feature-flag-extractor/angular-extractor/tsconfig.json",
                "assets": [
                    "libs/feature-flag-extractor/angular-extractor/builders.json",
                    "libs/feature-flag-extractor/angular-extractor/src/**/*.json"
                ]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": ["libs/feature-flag-extractor/angular-extractor/**/*.ts"]
            }
        }
    }
}
