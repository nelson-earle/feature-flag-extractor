{
    "name": "feature-flag-extractor",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/feature-flag-extractor/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/feature-flag-extractor",
                "main": "libs/feature-flag-extractor/src/index.ts",
                "tsConfig": "libs/feature-flag-extractor/tsconfig.lib.json",
                "assets": ["libs/feature-flag-extractor/src/**/*.json"]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "options": {
                "lintFilePatterns": [
                    "libs/feature-flag-extractor/**/*.ts",
                    "libs/feature-flag-extractor/**/*.json"
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [],
            "options": {
                "jestConfig": "libs/feature-flag-extractor/jest.config.ts"
            }
        }
    }
}
